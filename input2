int main(){
    decl
        int a, b, result;
    enddecl
    begin
        a = 1234;
        b = 2;
        result = a+b;
        write(result);
        result = a-b;
        write(result);
        result = a*b;
        write(result);
        result = a/b;
        write(result);
        result = a%b;
        write(result);
        return 1;
    end
}
